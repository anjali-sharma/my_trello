	<%= form_for(@board) do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div id="board-field">
		<%= f.label :title, "Create new board" %>
		<%= f.text_field :title, placeholder: "Title..." %>
	</div>
	<%= f.submit "Create", class: "btn btn-sm btn-primary", id: "create-board" %>
	<% end %>
